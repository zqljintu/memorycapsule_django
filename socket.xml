<uwsgi>
    <chdir>/memorycapsule/django/memorycapsule_django/</chdir>
    <module>memorycapsule_django.wsgi:application</module>
    <virtualenv>/memorycapsule/wszql</virtualenv>
    <master>true</master>
    <processes>2</processes>
    <harakiri>60</harakiri>
    <vacuum>true</vacuum>
    <max-requests>500000</max-requests>
    <socket>:8000</socket>
    <daemonize>uwsgi.log</daemonize>
    <py-autoreload>1</py-autoreload>
</uwsgi>
